{
  "name": "ApikoMarketplace",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "react-native run-android --mode=developmentdebug --appIdSuffix=development",
    "android:dev-release": "react-native run-android --mode=developmentrelease --appIdSuffix=development",
    "android:staging": "react-native run-android --mode=stagingdebug --appIdSuffix=staging",
    "android:staging-release": "react-native run-android --mode=stagingrelease --appIdSuffix=staging",
    "android:prod": "react-native run-android --mode=productiondebug --appIdSuffix=production",
    "android:prod-release": "react-native run-android --mode=productionrelease --appIdSuffix=production",
    "android:clear": "cd android && ./gradlew clean && cd ..",
    "ios:dev": "react-native run-ios --simulator='iPhone 8' --mode=Debug --scheme \"ApikoMarketplaceDev\"",
    "ios:dev-release": "react-native run-ios --simulator='iPhone 8' --mode=Release --scheme \"ApikoMarketplaceDev\"",
    "ios:staging": "react-native run-ios --simulator='iPhone 8' --mode=Debug --scheme \"ApikoMarketplaceStg\"",
    "ios:staging-release": "react-native run-ios --simulator='iPhone 8' --mode=Release --scheme \"ApikoMarketplaceStg\"",
    "ios:prod": "react-native run-ios --simulator='iPhone 8' --mode=Debug --scheme \"ApikoMarketplace\"",
    "ios:prod-release": "react-native run-ios --simulator='iPhone 8' --mode=Release --scheme \"ApikoMarketplace\"",
    "supabase:generate-types": "supabase gen types typescript --local > database.types.ts",
    "ios:pod-install": "npx pod-install ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.22.3",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/elements": "^1.3.22",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/stack": "^6.3.21",
    "@supabase/supabase-js": "^2.39.7",
    "i18next": "^23.10.0",
    "intl-pluralrules": "^2.0.1",
    "react": "18.2.0",
    "react-i18next": "^14.0.5",
    "react-native": "^0.73.4",
    "react-native-bootsplash": "^5.3.0",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-unistyles": "^2.3.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.3",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.21",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "^0.73.5",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "^18.2.0",
    "supabase": "^1.145.4",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
